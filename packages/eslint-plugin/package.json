{
	"name": "@mozisan/eslint-plugin",
	"version": "2.2.9",
	"description": "ESLint plugin",
	"keywords": [
		"eslint",
		"typescript"
	],
	"homepage": "https://github.com/mozisan/eslint-plugin/tree/main/packages/eslint-plugin",
	"bugs": {
		"url": "https://github.com/mozisan/eslint-plugin/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mozisan/eslint-plugin.git",
		"directory": "packages/eslint-plugin"
	},
	"license": "MIT",
	"author": "mozisan <mozisan@users.noreply.github.com>",
	"exports": {
		".": "./dist/plugin.js",
		"./package.json": "./package.json"
	},
	"main": "./dist/plugin.js",
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "run-s clean bundle",
		"bundle": "pkgroll",
		"clean": "rimraf dist",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"typecheck": "tsc --noEmit"
	},
	"prettier": "@mozisan/prettier-config",
	"dependencies": {
		"@next/eslint-plugin-next": "^14.0.4",
		"@typescript-eslint/eslint-plugin": "^6.18.0",
		"@typescript-eslint/parser": "^6.18.0",
		"@typescript-eslint/type-utils": "^6.18.0",
		"@typescript-eslint/utils": "^6.18.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-check-file": "^2.6.2",
		"eslint-plugin-functional": "^6.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-import-access": "^2.1.2",
		"eslint-plugin-no-relative-import-paths": "^1.5.3",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-perf": "^3.3.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-tailwindcss": "^3.13.1",
		"eslint-plugin-unicorn": "^50.0.1",
		"eslint-plugin-unused-imports": "^3.0.0"
	},
	"devDependencies": {
		"@mozisan/prettier-config": "^2.3.0",
		"@types/node": "20.10.6",
		"eslint": "^8.56.0",
		"rimraf": "^5.0.5",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"eslint": "^8",
		"tailwindcss": "^3"
	},
	"peerDependenciesMeta": {
		"tailwindcss": {
			"optional": true
		}
	},
	"publishConfig": {
		"access": "public"
	}
}
